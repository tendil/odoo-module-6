<odoo noupdate="1">
    <record id="res_partner_example" model="res.partner" forcecreate="True">
        <field name="name">Example Customer</field>
        <field name="country_id" ref="base.ua"/>
        <field name="image_1920" type="base64" file="../static/img/partner_example.png"/>
        <field name="comment" type="html">
            <div>
                <h3>This is an example customer.</h3>
            </div>
        </field>
        <field name="active" eval="True"/>
    </record>
</odoo>
